<p id="notice"><%= notice %></p>

<h1>Convalidacion Items</h1>

<table>
  <thead>
    <tr>
      <th>Convalidacions</th>
      <th>Asignaturas</th>
      <th>Asignatura procedencia clave</th>
      <th>Asignatura procedencia nombre</th>
      <th>Asignatura procedencia calificacion</th>
      <th>Asignatura procedencia creditos</th>
      <th>Asignatura local clave</th>
      <th>Asignatura local nombre</th>
      <th>Asignatura local creditos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @convalidacion_items.each do |convalidacion_item| %>
      <tr>
        <td><%= convalidacion_item.convalidacions %></td>
        <td><%= convalidacion_item.asignaturas %></td>
        <td><%= convalidacion_item.asignatura_procedencia_clave %></td>
        <td><%= convalidacion_item.asignatura_procedencia_nombre %></td>
        <td><%= convalidacion_item.asignatura_procedencia_calificacion %></td>
        <td><%= convalidacion_item.asignatura_procedencia_creditos %></td>
        <td><%= convalidacion_item.asignatura_local_clave %></td>
        <td><%= convalidacion_item.asignatura_local_nombre %></td>
        <td><%= convalidacion_item.asignatura_local_creditos %></td>
        <td><%= link_to 'Show', convalidacion_item %></td>
        <td><%= link_to 'Edit', edit_convalidacion_item_path(convalidacion_item) %></td>
        <td><%= link_to 'Destroy', convalidacion_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Convalidacion Item', new_convalidacion_item_path %>
